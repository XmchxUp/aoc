module Day1 exposing (..)

import Html exposing (text)


main =
    text ("Part1: " ++ String.fromInt part1 ++ "Part2: " ++ String.fromInt part2)


part1 =
    part1_helper inputs


part1_helper input =
    case String.uncons input of
        Just ( x, rest ) ->
            (case x of
                '(' ->
                    1

                ')' ->
                    -1

                _ ->
                    -- panic
                    0
            )
                + part1_helper rest

        Nothing ->
            0


part2 =
    part2_helper -1 0 0 inputs


part2_helper expected_floor pos cur_floor input =
    if expected_floor == cur_floor then
        pos

    else
        case String.uncons input of
            Just ( x, rest ) ->
                part2_helper expected_floor
                    (pos
                        + 1
                    )
                    (cur_floor
                        + (case x of
                            '(' ->
                                1

                            ')' ->
                                -1

                            _ ->
                                -- panic
                                0
                          )
                    )
                    rest

            Nothing ->
                -1


